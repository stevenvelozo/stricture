{
    "name": "stricture",
    "version": "1.0.38",
    "description": "Stricture MicroDDL JSON Parser",
    "main": "source/Stricture.js",
    "bin": {
        "stricture": "./bin/stricture"
    },
    "scripts": {
        "start": "node source/Stricture.js",
        "coverage": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -u tdd -R spec",
        "test": "./node_modules/mocha/bin/_mocha -u tdd -R spec",
        "tests": "npx mocha -u tdd --exit -R spec --grep",
        "build": "npx quack build",
        "docker-dev-build": "docker build ./ -f Dockerfile_LUXURYCode -t stricture-image:local",
        "docker-dev-run": "docker run -it -d --name stricture-dev -p 17715:8080 -p 13154:8086 -v \"$PWD/.config:/home/coder/.config\"  -v \"$PWD:/home/coder/stricture\" -u \"$(id -u):$(id -g)\" -e \"DOCKER_USER=$USER\" stricture-image:local",
        "docker-dev-shell": "docker exec -it stricture-dev /bin/bash"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/stevenvelozo/stricture.git"
    },
    "keywords": [
        "logging"
    ],
    "author": "Steven Velozo <steven@velozo.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/stevenvelozo/stricture/issues"
    },
    "devDependencies": {
        "chai": "3.4.1",
        "istanbul": "0.4.1",
        "mocha": "2.3.4"
    },
    "dependencies": {
        "async": "1.5.0",
        "fable": "~1.0.1",
        "jsonfile": "2.2.3",
        "line-by-line": "0.1.4",
        "minimist": "1.2.5",
        "mkdirp": "0.5.1",
        "underscore": "1.8.3",
        "yargs": "3.31.0"
    },
    "mocha": {
        "diff": true,
        "extension": [
            "js"
        ],
        "package": "./package.json",
        "reporter": "spec",
        "slow": "75",
        "timeout": "5000",
        "ui": "tdd",
        "watch-files": [
            "source/**/*.js",
            "test/**/*.js"
        ],
        "watch-ignore": [
            "lib/vendor"
        ]
    }
}
